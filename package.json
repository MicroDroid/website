{
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.30",
		"@fortawesome/free-brands-svg-icons": "^5.14.0",
		"@fortawesome/react-fontawesome": "^0.1.11",
		"clsx": "^1.1.1",
		"next": "9.5.3",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"sass": "^1.26.11"
	},
	"devDependencies": {
		"@types/node": "^14.11.2",
		"@types/react": "^16.9.49",
		"eslint-config-xo-react": "^0.23.0",
		"eslint-plugin-react": "^7.21.2",
		"eslint-plugin-react-hooks": "^4.1.2",
		"prettier": "2.0.4",
		"prettier-config-xo": "^1.0.3",
		"type-fest": "^0.16.0",
		"typescript": "3.9.7",
		"xo": "^0.33.1"
	},
	"name": "website",
	"private": true,
	"scripts": {
		"build": "next build",
		"dev": "next dev",
		"export": "next export",
		"lint": "xo",
		"start": "next start",
		"style": "prettier --ignore-path .gitignore --check ."
	},
	"version": "4.0.0",
	"xo": {
		"extends": [
			"xo",
			"xo/esnext",
			"xo/browser",
			"xo-react"
		],
		"ignores": [
			"public"
		],
		"overrides": [
			{
				"extends": [
					"xo"
				],
				"files": "*.config.js"
			}
		],
		"prettier": true,
		"rules": {
			"import/no-unassigned-import": [
				"error",
				{
					"allow": [
						"firebase/*",
						"firebase",
						"globals"
					]
				}
			],
			"react/no-unescaped-entities": "off",
			"react/prop-types": "off"
		}
	}
}
